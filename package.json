    "get-stdin": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-config-notninja": "^0.2.3",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1"
  },
  "bin": {
    "native2ascii": "bin/native2ascii"
  },
  "main": "src/native2ascii.js",
  "scripts": {
